namespace HiMTA_Kuzmich;
using HiMTA_Kuzmich::Address;
using HiMTA_Kuzmich::Car;

entity User {
    @Comment: 'Id of User'
    key usid : String(4);
    @Comment: 'Description'
    name : String(100);

    toCars : Association[*] to Car on toCars.usid = usid;
    toAddress : Association[1] to Address on toAddress.usid = usid;
};